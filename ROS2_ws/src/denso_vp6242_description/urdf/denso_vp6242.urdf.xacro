<?xml version="1.0" encoding="UTF-8"?>
<robot name="denso_vp6242" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:arg name="gripper_type" default="taiyo"/>
	<xacro:property name="gripper_type" value="$(arg gripper_type)" />
	<xacro:property name="scale_value" value="100.0"/>
	
	<link name="base_link">
		<visual>
			<origin rpy="-1.570796 3.141592 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/visual/robot/base_link.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="-1.570796 3.141592 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/collision/robot/base_link.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</collision>
	</link>

	<link name="link_1">
		<visual>
			<origin rpy="-1.570796 3.141592 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/visual/robot/link_1.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="-1.570796 3.141592 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/collision/robot/link_1.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</collision>
	</link>

	<joint name="joint_1" type="revolute">
		<parent link="base_link"/>
		<child link="link_1" />
		<origin rpy="0 0 0" xyz="0.0 0.0 ${scale_value*0.155}"/>
		<axis xyz="0 0 1"/>
		<limit effort="${scale_value * 100.0}" lower="-2.792526803" upper="2.792526803" velocity="0.403171057210690"/>
	</joint>

	<link name="link_2">
		<visual>
			<origin rpy="-1.570796 3.141592 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/visual/robot/link_2.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="-1.570796 3.141592 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/collision/robot/link_2.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</collision>
	</link>

	<joint name="joint_2" type="revolute">
		<parent link="link_1"/>
		<child link="link_2"/>
		<origin rpy="0 0 0" xyz="0.0 0.0 ${scale_value*0.125}"/>
		<axis xyz="-0.000000 1.000000 -0.000000" />
		<limit effort="${scale_value * 100.0}" lower="-2.094395102" upper="2.094395102" velocity="0.392175482923126" />
	</joint>

	<link name="link_3">
		<visual>
			<origin rpy="-1.570796 1.570796 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/visual/robot/link_3.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="-1.570796 1.570796 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/collision/robot/link_3.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</collision>
	</link>

	<joint name="joint_3" type="revolute">
		<parent link="link_2"/>
		<child link="link_3"/>
		<origin rpy="0 0 0" xyz="0.0 0.0 ${scale_value*0.210}"/>
		<axis xyz="-0.000000 1.000000 -0.000000" />
		<limit effort="${scale_value * 100.0}" lower="0.331612558" upper="2.792526803" velocity="0.725707902979242" />
	</joint>

	<link name="link_4">
		<visual>
			<origin rpy="0 1.570796 1.570796" xyz="0 0 ${scale_value*0.085}"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/visual/robot/link_4.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 1.570796 1.570796" xyz="0 0 ${scale_value*0.085}"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/collision/robot/link_4.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</collision>
	</link>

	<joint name="joint_4" type="revolute">
		<parent link="link_3"/>
		<child link="link_4"/>
		<origin rpy="0 0 0" xyz="${scale_value*(-0.075)} 0.0 0.0"/>
		<axis xyz="-0.000000 -0.000000 1.000000" />
		<limit effort="${scale_value * 100.0}" lower="-2.792526803" upper="2.792526803" velocity="0.739146938219599" />
	</joint>

	<link name="link_5">
		<visual>
			<origin rpy="0 1.570796 1.570796" xyz="0 0 0.0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/visual/robot/link_5.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</visual>
		<collision>
			<origin rpy="0 1.570796 1.570796" xyz="0 0 0.0"/>
			<geometry>
				<mesh filename="package://denso_vp6242_description/meshes/collision/robot/link_5.dae" scale="${scale_value} ${scale_value} ${scale_value}"/>
			</geometry>
		</collision>
	</link>

	<joint name="joint_5" type="revolute">
		<parent link="link_4"/>
		<child link="link_5"/>
		<origin rpy="0 0 0" xyz="0.0 0.0 ${scale_value*0.210}"/>
		<axis xyz="-0.000000 1.000000 -0.000000" />
		<limit effort="${scale_value * 100.0}" lower="-2.094395102" upper="2.094395102" velocity="0.739146938219599" />
	</joint>

	<link name="link_6">
		<visual>
			<geometry>
				<cylinder length="${scale_value*0.01}" radius="${scale_value*0.0205}"/>
			</geometry>
			<origin rpy="0 0 0" xyz="0.0 0.0 ${scale_value*(-0.005)}"/>
			<material name="plate">
				<color rgba="0.1 0.1 0.1 0.8"/>
			</material>
		</visual>
	</link>

	<joint name="joint_6" type="revolute">
		<parent link="link_5"/>
		<child link="link_6"/>
		<origin rpy="0 0 0" xyz="0.0 0.0 ${scale_value*0.07}"/>
		<axis xyz="-0.000000 -0.000000 1.000000" />
		<limit effort="${scale_value * 100.0}" lower="-2.96706" upper="2.96706" velocity="1.109331272567600" />
	</joint>

	<xacro:if value="${gripper_type != 'none'}">
		<xacro:include filename="$(find denso_vp6242_description)/urdf/${gripper_type}.urdf.xacro" />
	</xacro:if>

</robot>
